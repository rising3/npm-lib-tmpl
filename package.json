{
  "name": "@rising3/npm-lib-tmpl",
  "version": "0.0.0",
  "description": "This repository is a template for JavaScript npm library.",
  "main": "./lib/index.js",
  "repository": "https://github.com/rising3/npm-lib-tmpl.git",
  "author": "Michio Nakagawa <michio.nakagawa@gmail.com>",
  "bugs": {
    "url": "https://github.com/rising3/npm-lib-tmpl/issues"
  },
  "contributors": [
    "rising3 (https://twitter.com/@rising3)"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    "@semantic-release/git",
    "@semantic-release-npm-github-publish"
  ],
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "semantic-release-npm-github-publish": "^1.4.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "lint": "eslint 'lib/**/*.js'",
    "lint:fix": "eslint 'lib/**/*.js' --fix",
    "jsdoc": "jsdoc -c jsdoc.config.json -d ./dist/docs ./lib/"
  }
}
